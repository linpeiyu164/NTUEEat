( userComment?\n                <CommentedBox username={user.username} rating={userRate} content={userComment}/>\n                : <div className={classes.typeInComment}>\n                    <div style={{display: 'flex'}}>\n                        <Avatar style={{right: '5px'}} src=\"\"/>\n                        <div>\n                            <span style={{display: 'block'}}>{user.username}</span>\n                            <RateStar \n                                handleSelectRate={setRate}\n                                style={{display: 'block'}}\n                            />\n                        </div>\n                    </div>\n                    \n                    <TextField onChange={handleChange} value={typeIn}/>\n                    <IconButton onClick={handleSubmit}>\n                        <SendIcon />\n                    </IconButton>\n                </div>) : <div>先登入才可以評論ㄛ～</div>\n        )\n    }\n    // remain: avatar\n    function Comments (props) {\n        let comments = []\n        if (props.comments) {\n            if (user) {//console.log(\"comments: \", props)\n                props.comments.forEach(comment => {\n                    if (comment.username !== user.username){\n                        \n                        comments.push(<CommentedBox username={comment.username} rating={comment.rating} content={comment.content} />)\n                        \n                        console.log('comments: ', comments)\n                    } else {\n                        setUserComment(comment.content)\n                        setUserRate(comment.rating)\n                    }\n                })\n            } else {\n            props.comments.forEach(comment => {\n                comments.push(<CommentedBox username={comment.username} rating={comment.rating} content={comment.content} />)\n                \n                console.log('comments: ', comments)\n            })\n        }\n        } \n        return (\n            <List className={classes.commentList} >\n                {comments}\n            </List>\n        )\n        \n    }\n    return ( \n        <div className={classes.root}>\n            \n            <TypeIn />\n            <Comments comments={props.data&&props.data.comments}/>\n        </div>\n    )\n}\n\nexport default Review;",{"ruleId":"140","replacedBy":"141"},{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","severity":1,"message":"145","line":46,"column":10,"nodeType":"146","messageId":"147","endLine":46,"endColumn":18},{"ruleId":"148","severity":1,"message":"149","line":141,"column":25,"nodeType":"150","endLine":141,"endColumn":42},{"ruleId":"144","severity":1,"message":"151","line":3,"column":34,"nodeType":"146","messageId":"147","endLine":3,"endColumn":40},{"ruleId":"152","severity":1,"message":"153","line":107,"column":7,"nodeType":"154","endLine":107,"endColumn":9,"suggestions":"155"},{"ruleId":"144","severity":1,"message":"156","line":4,"column":31,"nodeType":"146","messageId":"147","endLine":4,"endColumn":41},{"ruleId":"144","severity":1,"message":"157","line":7,"column":22,"nodeType":"146","messageId":"147","endLine":7,"endColumn":28},{"ruleId":"152","severity":1,"message":"158","line":35,"column":15,"nodeType":"159","endLine":50,"endColumn":6},{"ruleId":"160","severity":1,"message":"161","line":64,"column":24,"nodeType":"162","endLine":67,"endColumn":17},{"ruleId":"144","severity":1,"message":"163","line":2,"column":42,"nodeType":"146","messageId":"147","endLine":2,"endColumn":48},{"ruleId":"144","severity":1,"message":"164","line":7,"column":45,"nodeType":"146","messageId":"147","endLine":7,"endColumn":49},{"ruleId":"144","severity":1,"message":"165","line":21,"column":8,"nodeType":"146","messageId":"147","endLine":21,"endColumn":12},{"ruleId":"152","severity":1,"message":"158","line":49,"column":13,"nodeType":"159","endLine":59,"endColumn":4},{"ruleId":"144","severity":1,"message":"166","line":3,"column":31,"nodeType":"146","messageId"